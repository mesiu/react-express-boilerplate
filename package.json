{
  "name": "react-express-boilerplate",
  "description": "Boilerplate for a React project running with an Express server",
  "scripts": {
    "build": "webpack",
    "lint": "eslint . --ignore-path .gitignore",
    "serve": "node ./server",
    "start": "npm run serve",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mesiu/react-express-boilerplate.git"
  },
  "author": "Melissa Siu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mesiu/react-express-boilerplate/issues"
  },
  "homepage": "https://github.com/mesiu/react-express-boilerplate#readme",
  "dependencies": {
    "compression": "1.6.2",
    "express": "4.15.3",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-hot-loader": "3.0.0-beta.7",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
